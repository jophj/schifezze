<div class="container">
  <div class="row">
    <div class="col s6">
      {% include 'recap.twig' with recap.jop only  %}
      {% include 'junkList.twig' with {schifezzeList: schifezzeList.jop} only  %}
    </div>
    <div class="col s6">
      {% include 'recap.twig' with recap.silvia only  %}
      {% include 'junkList.twig' with {schifezzeList: schifezzeList.silvia} only  %}
    </div>
  </div>
  <div id="chartContainer" style="width:100%; height:400px;"></div>

  <script type="text/javascript">
    $(function () { 
      $('#chartContainer').highcharts({
          chart: {
              type: 'bar'
          },
          title: {
              text: 'Fruit Consumption'
          },
          xAxis: {
              categories: ['Apples', 'Bananas', 'Oranges']
          },
          yAxis: {
              title: {
                  text: 'Fruit eaten'
              }
          },
          series: [{
              name: 'Jane',
              data: [1, 0, 4]
          }, {
              name: 'John',
              data: [5, 7, 3]
          }]
        });
    });
  </script>
</div>